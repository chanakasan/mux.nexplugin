#!/usr/bin/env bash

set -e

main() {
  local a1="$1"
  local fn=_$a1
  if [ -z "$a1" ]; then
    tmux-ensure
  elif [[ $(type -t $fn ) == function ]]; then
    $fn "${@:2}"
  else
    _start $v1
  fi
}

_desk() {
  tmux-aoc "desktop"
}

_list() {
  tmux list-sessions
}

_example() {
  local example=$nex_mux_path/src/example.sh
  vi $example
}

_new() {
  local name=$(tmux-new-session-name)
  tmux-aoc $name
}

_create() {
 vi $nex_mux_path/src/example.sh
}

main "$@"
